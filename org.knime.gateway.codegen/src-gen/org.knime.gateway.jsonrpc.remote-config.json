{
    "lang" : "org.knime.gateway.codegen.GatewayCodegen",
    "inputSpec" : "../org.knime.gateway/api/gateway.yaml",
    "outputDir" : "../../knime-com-gateway/com.knime.gateway.jsonrpc.remote",
    "modelPackage" : "org.knime.gateway.v0.entity",
    "apiPackage"   : "com.knime.gateway.jsonrpc.remote.service",
    "fullJavaUtil" : true,
    "dateLibrary" : "java8",
    "sourceFolder" : "src/generated",
    "templateDir" : "src-gen/templates",
    "additionalProperties" : {
        "codegenName" : "json-rpc-wrappers",
        "apiTemplateFile" : "serviceJsonRpcImpl.mustache",
        "apiNamePattern" : "JsonRpc##name##ServiceWrapper",
        "modelNamePattern" : "##name##Ent",
        "supportingFiles" : [
            {
                "templateFile" : "wrapWithJsonRpcService.mustache",
                "destinationFileName" : "WrapWithJsonRpcService.java",
                "package" : "com.knime.gateway.jsonrpc.remote.service.util"
            }        
        
        ]
    }   
}