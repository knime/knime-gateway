                    <url>${knime.dev.p2}</url>
                </repository>
                <repository>
                    <id>p2-knime-product</id>
                    <layout>p2</layout>
                    <url>${knime.product.p2}</url>
                </repository>
                <repository>
                    <id>p2-knime-json</id>
                    <layout>p2</layout>
