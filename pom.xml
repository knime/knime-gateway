<?xml version="1.0" encoding="UTF-8"?><project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">

    <modelVersion>4.0.0</modelVersion>

    <parent>
        <groupId>org.knime.maven</groupId>
        <artifactId>ap.parent</artifactId>
        <version>5.3.0</version>
    </parent>

    <groupId>org.knime</groupId>
    <artifactId>knime-gateway</artifactId>
    <version>${revision}${changelist}</version>
    <packaging>pom</packaging>

    <properties>
        <revision>5.3.4</revision>
        <changelist>-SNAPSHOT</changelist>
        <feature-branch-extension.branchListProperty>git.branch</feature-branch-extension.branchListProperty>
        <feature-branch-extension.appendArtifactoryProperties>false</feature-branch-extension.appendArtifactoryProperties>
        <knime.sonar-env>Sonarcloud</knime.sonar-env>
        <sonar.exclusions>
            org.knime.gateway.api/src/generated/**/*,
            org.knime.gateway.impl/src/generated/**/*,
            org.knime.gateway.impl.jsonrpc/src/generated/**/*,
            org.knime.gateway.json/src/generated/**/*
        </sonar.exclusions>
    </properties>

    <profiles>
        <profile>
            <id>build</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <modules>
                <module>org.knime.gateway.api</module>
                <module>org.knime.gateway.json</module>
                <module>org.knime.gateway.impl</module>
                <module>org.knime.gateway.impl.jsonrpc</module>
                <module>org.knime.gateway.testing.helper</module>
                <module>com.github.briandilley.jsonrpc4j.clfix</module>
                <module>org.knime.update.gateway</module>
                <module>org.knime.features.gateway</module>
            </modules>
        </profile>
        <profile>
            <id>test</id>
            <modules>
                <module>org.knime.gateway.api</module>
                <module>org.knime.gateway.api.tests</module>
                <module>org.knime.gateway.json</module>
                <module>org.knime.gateway.json.tests</module>
                <module>org.knime.gateway.impl</module>
                <module>org.knime.gateway.impl.tests</module>
                <module>org.knime.gateway.impl.jsonrpc</module>
                <module>org.knime.gateway.impl.jsonrpc.tests</module>
                <module>org.knime.gateway.testing.helper</module>
            </modules>
            <repositories>
                <repository>
                    <id>p2-knime-core</id>
                    <layout>p2</layout>
                    <url>${knime.core.p2}</url>
                </repository>
                <repository>
                    <id>p2-knime-gateway</id>
