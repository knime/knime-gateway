                    <layout>p2</layout>
                    <url>${knime.shared.p2}</url>
                </repository>
                <repository>
                    <id>p2-knime-core-ui</id>
                    <url>${knime.core.ui.p2}</url>
                    <layout>p2</layout>
                </repository>
                <repository>
                    <id>p2-knime-workbench</id>
                    <layout>p2</layout>
                    <url>${knime.workbench.p2}</url>
                </repository>
                <repository>
                    <id>p2-knime-base</id>
